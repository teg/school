- form_for(student) do |f|

  = f.error_messages

  %p
    = f.label :name
    %br
    = f.text_field :name

  %p
    = f.label :email
    %br
    = f.text_field :email

  %p
    = f.label :skype
    %br
    = f.text_field :skype
    
  %p
    = f.label :password
    %br
    = f.password_field :password
  
  %p
    = f.label :password_confirmation
    %br
    = f.password_field :password_confirmation

  %p
    = f.submit button_label
